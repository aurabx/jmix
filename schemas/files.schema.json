{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://aurabox.cloud/jmix/schemas/files.schema.json",
  "title": "JMIX files.json",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "file": {"type": "string"},
      "hash": {"type": "string"},
      "size_bytes": {"type": "integer", "minimum": 0}
    },
    "required": ["file"]
  }
}
